src\ejs\ejs.h: $(DEPS_29)

#
#   ejsLib.obj
#
DEPS_30 = $(DEPS_30) src\ejs\ejs.h
DEPS_30 = $(DEPS_30) build\$(CONFIG)\inc\mpr.h
DEPS_30 = $(DEPS_30) build\$(CONFIG)\inc\pcre.h
DEPS_30 = $(DEPS_30) build\$(CONFIG)\inc\me.h

build\$(CONFIG)\obj\ejsLib.obj: \
    src\ejs\ejsLib.c $(DEPS_30)
	@echo .. [Compile] build\$(CONFIG)\obj\ejsLib.obj
	"$(CC)" -c -Fo$(BUILD)\obj\ejsLib.obj -Fd$(BUILD)\obj\ejsLib.pdb $(CFLAGS) $(DFLAGS) -DME_COM_OPENSSL_PATH="$(ME_COM_OPENSSL_PATH)" $(IFLAGS) "-I$(ME_COM_OPENSSL_PATH)\include" src\ejs\ejsLib.c $(LOG)

!IF "$(ME_COM_EJS)" == "1"
#
#   libejs
#
!IF "$(ME_COM_HTTP)" == "1"
DEPS_31 = $(DEPS_31) build\$(CONFIG)\bin\libhttp.dll
!ENDIF
!IF "$(ME_COM_PCRE)" == "1"
DEPS_31 = $(DEPS_31) build\$(CONFIG)\bin\libpcre.dll
!ENDIF
DEPS_31 = $(DEPS_31) build\$(CONFIG)\bin\libmpr.dll
!IF "$(ME_COM_ZLIB)" == "1"
DEPS_31 = $(DEPS_31) build\$(CONFIG)\bin\libzlib.dll
!ENDIF
DEPS_31 = $(DEPS_31) build\$(CONFIG)\inc\ejs.h
DEPS_31 = $(DEPS_31) build\$(CONFIG)\inc\ejs.slots.h
DEPS_31 = $(DEPS_31) build\$(CONFIG)\inc\ejsByteGoto.h
DEPS_31 = $(DEPS_31) build\$(CONFIG)\obj\ejsLib.obj

!IF "$(ME_COM_ZLIB)" == "1"
LIBS_31 = $(LIBS_31) libzlib.lib
!ENDIF
!IF "$(ME_COM_HTTP)" == "1"
LIBS_31 = $(LIBS_31) libhttp.lib
!ENDIF
!IF "$(ME_COM_PCRE)" == "1"
LIBS_31 = $(LIBS_31) libpcre.lib
!ENDIF
LIBS_31 = $(LIBS_31) libmpr.lib
!IF "$(ME_COM_EST)" == "1"
LIBS_31 = $(LIBS_31) libestssl.lib
!ENDIF
!IF "$(ME_COM_EST)" == "1"
LIBS_31 = $(LIBS_31) libest.lib
!ENDIF
!IF "$(ME_COM_OPENSSL)" == "1"
LIBS_31 = $(LIBS_31) libopenssl.lib
LIBPATHS_31 = $(LIBPATHS_31) -libpath:$(ME_COM_OPENSSL_PATH)
!ENDIF
!IF "$(ME_COM_OPENSSL)" == "1"
LIBS_31 = $(LIBS_31) libeay32.lib
LIBPATHS_31 = $(LIBPATHS_31) -libpath:$(ME_COM_OPENSSL_PATH)
!ENDIF
!IF "$(ME_COM_OPENSSL)" == "1"
LIBS_31 = $(LIBS_31) libssleay32.lib
LIBPATHS_31 = $(LIBPATHS_31) -libpath:$(ME_COM_OPENSSL_PATH)
!ENDIF

build\$(CONFIG)\bin\libejs.dll: $(DEPS_31)
	@echo ..... [Link] build\$(CONFIG)\bin\libejs.dll
	"$(LD)" -dll -out:$(BUILD)\bin\libejs.dll -entry:$(ENTRY) $(LDFLAGS) $(LIBPATHS)  "$(BUILD)\obj\ejsLib.obj" $(LIBPATHS_31) $(LIBS_31) $(LIBS_31) $(LIBS)  $(LOG)
!ENDIF

#
#   ejsc.obj
#
DEPS_32 = $(DEPS_32) src\ejs\ejs.h

build\$(CONFIG)\obj\ejsc.obj: \
    src\ejs\ejsc.c $(DEPS_32)
	@echo .. [Compile] build\$(CONFIG)\obj\ejsc.obj
	"$(CC)" -c -Fo$(BUILD)\obj\ejsc.obj -Fd$(BUILD)\obj\ejsc.pdb $(CFLAGS) $(DFLAGS) -DME_COM_OPENSSL_PATH="$(ME_COM_OPENSSL_PATH)" $(IFLAGS) "-I$(ME_COM_OPENSSL_PATH)\include" src\ejs\ejsc.c $(LOG)

!IF "$(ME_COM_EJS)" == "1"
#
