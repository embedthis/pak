#
DEPS_33 = $(DEPS_33) build\$(CONFIG)\bin\libejs.dll
DEPS_33 = $(DEPS_33) build\$(CONFIG)\obj\ejsc.obj

LIBS_33 = $(LIBS_33) libejs.lib
!IF "$(ME_COM_ZLIB)" == "1"
LIBS_33 = $(LIBS_33) libzlib.lib
!ENDIF
!IF "$(ME_COM_HTTP)" == "1"
LIBS_33 = $(LIBS_33) libhttp.lib
!ENDIF
!IF "$(ME_COM_PCRE)" == "1"
LIBS_33 = $(LIBS_33) libpcre.lib
!ENDIF
LIBS_33 = $(LIBS_33) libmpr.lib
!IF "$(ME_COM_EST)" == "1"
LIBS_33 = $(LIBS_33) libestssl.lib
!ENDIF
!IF "$(ME_COM_EST)" == "1"
LIBS_33 = $(LIBS_33) libest.lib
